#include <iostream>
#include <queue>

#define ll longlong;
using namespace std;

int main(){
    cin.tie(NULL);
    cout.tie(NULL);
    //입력
    int n, a, present;
    priority_queue<int>pq;
    cin >> n;
    //구현 및 출력
    for(int i=0; i<n; i++){
        cin >> a;
        if(a==0){
            if(pq.empty()){
                cout << -1 << "\n";
            }else{
                cout << pq.top() << "\n"; //제일 큰 값 출력
                pq.pop(); //제일 큰 값 pop
            }
        }else{
            for(int i=0; i<a; i++){
                cin >> present;
                pq.push(present);
            }
        }
    }
    return 0;

}
